{
    "manifest_version": 2,
    "name": "Tab-Save",
    "version": "1.3.0",
    "description": "Get open tab URLs for window, or load a list of URLs into new tabs on this window.",
    "icons":
    {
        "128": "icon128.png"
    },
    "browser_action":
    {
        "default_icon": "icon128.png",
        "default_title": "Tab-Save",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": ["tabs"]
}